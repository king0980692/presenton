            "maxLength": 60
          },
          "subtitle": {
            "type": "string",
            "minLength": 20,
            "maxLength": 200
          },
          "items": {
            "minItems": 1,
            "maxItems": 4,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "year": {
                  "type": "string",
                  "minLength": 3,
                  "maxLength": 6
                },
                "heading": {
                  "type": "string",
                  "minLength": 3,
                  "maxLength": 28
                },
                "body": {
                  "type": "string",
                  "minLength": 10,
                  "maxLength": 160
                },
                "icon": {
                  "type": "object",
                  "properties": {
                    "__icon_url__": {
                      "type": "string"
                    },
                    "__icon_query__": {
                      "type": "string",
                      "minLength": 0,
                      "maxLength": 80
                    }
                  },
                  "required": [
                    "__icon_url__",
                    "__icon_query__"
                  ],
                  "additionalProperties": false
                }
              },
              "required": [
                "year",
                "heading",
                "body",
                "icon"
              ],
              "additionalProperties": false
            }
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          }
        },
        "required": [
          "title",
          "subtitle",
          "items",
          "website"
        ],
        "additionalProperties": false
      }
    }
  ]
}