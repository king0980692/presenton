{
  "name": "swift",
  "ordered": false,
  "slides": [
    {
      "id": "swift:bullet-with-icons-title-description",
      "name": "Bullet With Icons Title Description",
      "description": "Bullet with icons with title and description and title and description for whole",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "minLength": 3,
            "maxLength": 60
          },
          "sideHeading": {
            "type": "string",
            "minLength": 0,
            "maxLength": 60
          },
          "sideParagraph": {
            "type": "string",
            "minLength": 0,
            "maxLength": 300
          },
          "items": {
            "minItems": 3,
            "maxItems": 4,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "icon": {
                  "type": "object",
                  "properties": {
                    "__icon_url__": {
                      "description": "URL to icon",
                      "type": "string"
                    },
                    "__icon_query__": {
                      "type": "string",
                      "minLength": 5,
                      "maxLength": 20
                    }
                  },
                  "required": [
                    "__icon_url__",
                    "__icon_query__"
                  ],
                  "additionalProperties": false
                },
                "title": {
                  "type": "string",
                  "minLength": 3,
                  "maxLength": 40
                },
                "description": {
                  "type": "string",
                  "minLength": 0,
                  "maxLength": 160
                }
              },
              "required": [
                "icon",
                "title",
                "description"
              ],
              "additionalProperties": false
            }
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          }
        },
        "required": [
          "title",
          "sideHeading",
          "sideParagraph",
          "items",
          "website"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:icon-bullet-list-description-slide",
      "name": "Icon Bullet List Description",
      "description": "Bullet list with title, description, and icon",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "minLength": 8,
            "maxLength": 48
          },
          "description": {
            "type": "string",
            "minLength": 30,
            "maxLength": 200
          },
          "features": {
            "minItems": 3,
            "maxItems": 4,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "minLength": 4,
                  "maxLength": 28
                },
                "body": {
                  "type": "string",
                  "minLength": 20,
                  "maxLength": 140
                },
                "icon": {
                  "type": "object",
                  "properties": {
                    "__icon_url__": {
                      "type": "string"
                    },
                    "__icon_query__": {
                      "type": "string",
                      "minLength": 0,
                      "maxLength": 80
                    }
                  },
                  "required": [
                    "__icon_url__",
                    "__icon_query__"
                  ],
                  "additionalProperties": false
                }
              },
              "required": [
                "title",
                "body",
                "icon"
              ],
              "additionalProperties": false
            }
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          }
        },
        "required": [
          "title",
          "description",
          "features",
          "website"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:image-list-description-slide",
      "name": "Image List Description",
      "description": "List of Images with subtitle and description with one description for the entire page",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "titleLine1": {
            "type": "string",
            "minLength": 3,
            "maxLength": 24
          },
          "titleLine2": {
            "type": "string",
            "minLength": 3,
            "maxLength": 24
          },
          "description": {
            "type": "string",
            "minLength": 20,
            "maxLength": 200
          },
          "items": {
            "minItems": 3,
            "maxItems": 6,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "minLength": 2,
                  "maxLength": 40
                },
                "description": {
                  "type": "string",
                  "minLength": 10,
                  "maxLength": 140
                },
                "image": {
                  "type": "object",
                  "properties": {
                    "__image_url__": {
                      "type": "string",
                      "format": "uri"
                    },
                    "__image_prompt__": {
                      "type": "string",
                      "minLength": 0,
                      "maxLength": 120
                    }
                  },
                  "required": [
                    "__image_url__",
                    "__image_prompt__"
                  ],
                  "additionalProperties": false
                }
              },
              "required": [
                "title",
                "description",
                "image"
              ],
              "additionalProperties": false
            }
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          }
        },
        "required": [
          "titleLine1",
          "titleLine2",
          "description",
          "items",
          "website"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:IntroSlideLayout",
      "name": "Intro Slide Layout",
      "description": "Intro slide with header, title, subtitle, body, image. If used for last slide, then intro card should be disabled.",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "description": "Main slide title",
            "type": "string",
            "minLength": 12,
            "maxLength": 68
          },
          "subtitlePrefix": {
            "type": "string",
            "minLength": 3,
            "maxLength": 40
          },
          "subtitleAccent": {
            "type": "string",
            "minLength": 3,
            "maxLength": 40
          },
          "paragraph": {
            "type": "string",
            "minLength": 40,
            "maxLength": 200
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          },
          "introCard": {
            "type": "object",
            "properties": {
              "enabled": {
                "type": "boolean"
              },
              "name": {
                "type": "string",
                "minLength": 3,
                "maxLength": 40
              },
              "date": {
                "type": "string",
                "minLength": 4,
                "maxLength": 40
              }
            },
            "required": [
              "enabled",
              "name",
              "date"
            ],
            "additionalProperties": false
          },
          "media": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "const": "image"
              },
              "image": {
                "type": "object",
                "properties": {
                  "__image_url__": {
                    "type": "string",
                    "format": "uri"
                  },
                  "__image_prompt__": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 120
                  }
                },
                "required": [
                  "__image_url__",
                  "__image_prompt__"
                ],
                "additionalProperties": false
              }
            },
            "required": [
              "type",
              "image"
            ],
            "additionalProperties": false
          }
        },
        "required": [
          "title",
          "subtitlePrefix",
          "subtitleAccent",
          "paragraph",
          "website",
          "introCard",
          "media"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:MetricsNumbers",
      "name": "Metrics Numbers",
      "description": "Swift: Our Impact in Numbers with three stacked metric cards",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "minLength": 8,
            "maxLength": 60
          },
          "leftTitle": {
            "type": "string",
            "minLength": 6,
            "maxLength": 40
          },
          "leftBody": {
            "type": "string",
            "minLength": 30,
            "maxLength": 220
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          },
          "metrics": {
            "minItems": 1,
            "maxItems": 4,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "value": {
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 8
                },
                "line1": {
                  "type": "string",
                  "minLength": 2,
                  "maxLength": 22
                },
                "line2": {
                  "type": "string",
                  "minLength": 0,
                  "maxLength": 22
                },
                "description": {
                  "type": "string",
                  "minLength": 10,
                  "maxLength": 140
                }
              },
              "required": [
                "value",
                "line1",
                "line2",
                "description"
              ],
              "additionalProperties": false
            }
          }
        },
        "required": [
          "title",
          "leftTitle",
          "leftBody",
          "website",
          "metrics"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:simple-bullet-points-layout",
      "name": "Simple Bullet Points",
      "description": "Bullet Points with title and description",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "minLength": 4,
            "maxLength": 36
          },
          "statement": {
            "type": "string",
            "minLength": 20,
            "maxLength": 260
          },
          "points": {
            "minItems": 1,
            "maxItems": 4,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "minLength": 6,
                  "maxLength": 60
                },
                "body": {
                  "type": "string",
                  "minLength": 30,
                  "maxLength": 220
                }
              },
              "required": [
                "title",
                "body"
              ],
              "additionalProperties": false
            }
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          }
        },
        "required": [
          "title",
          "statement",
          "points",
          "website"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:SwiftTableOfContents",
      "name": "Table Of Contents",
      "description": "Swift: Table of contents with up to 10 items (title + description)",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "minLength": 3,
            "maxLength": 60
          },
          "items": {
            "minItems": 1,
            "maxItems": 10,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "minLength": 3,
                  "maxLength": 40
                },
                "description": {
                  "type": "string",
                  "minLength": 0,
                  "maxLength": 60
                }
              },
              "required": [
                "title",
                "description"
              ],
              "additionalProperties": false
            }
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          }
        },
        "required": [
          "title",
          "items",
          "website"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:tableorChart",
      "name": "Table Or Chart",
      "description": "Swift: Generic data table with option to render a chart (bar, horizontalBar, line, pie)",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          },
          "description": {
            "type": "string",
            "minLength": 20,
            "maxLength": 220
          },
          "mode": {
            "type": "string",
            "enum": [
              "table",
              "chart"
            ]
          },
          "columns": {
            "minItems": 2,
            "maxItems": 10,
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1,
              "maxLength": 40
            }
          },
          "rows": {
            "minItems": 1,
            "maxItems": 30,
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "cells": {
                  "description": "Row cells; count should match columns length",
                  "minItems": 2,
                  "maxItems": 10,
                  "type": "array",
                  "items": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 200
                  }
                }
              },
              "required": [
                "cells"
              ],
              "additionalProperties": false
            }
          },
          "chart": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "bar",
                  "horizontalBar",
                  "line",
                  "pie"
                ]
              },
              "data": {
                "minItems": 3,
                "maxItems": 12,
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string",
                      "minLength": 1,
                      "maxLength": 12
                    },
                    "value": {
                      "type": "number",
                      "minimum": 0,
                      "maximum": 1000000
                    }
                  },
                  "required": [
                    "label",
                    "value"
                  ],
                  "additionalProperties": false
                }
              },
              "primaryColor": {
                "type": "string"
              },
              "gridColor": {
                "type": "string"
              },
              "pieColors": {
                "minItems": 1,
                "maxItems": 10,
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "showLabels": {
                "type": "boolean"
              }
            },
            "required": [
              "type",
              "data",
              "primaryColor",
              "gridColor",
              "pieColors",
              "showLabels"
            ],
            "additionalProperties": false
          },
          "website": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60
          }
        },
        "required": [
          "title",
          "description",
          "mode",
          "columns",
          "rows",
          "chart",
          "website"
        ],
        "additionalProperties": false
      }
    },
    {
      "id": "swift:Timeline",
      "name": "Timeline",
      "description": "Timeline of cards with title, subtitle banner",
      "json_schema": {
