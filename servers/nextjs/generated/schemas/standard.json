                    "maxLength": 30
                  },
                  "actionIcon": {
                    "type": "object",
                    "properties": {
                      "__icon_url__": {
                        "description": "URL to icon",
                        "type": "string"
                      },
                      "__icon_query__": {
                        "description": "Query used to search the icon. Max 5 words",
                        "type": "string",
                        "minLength": 3,
                        "maxLength": 24
                      }
                    },
                    "required": [
                      "__icon_url__",
                      "__icon_query__"
                    ],
                    "additionalProperties": false
                  }
                },
                "required": [
                  "websiteText",
                  "actionIcon"
                ],
                "additionalProperties": false
              }
            },
            "required": [
              "backgroundImage",
              "centerIcon",
              "websiteBar"
            ],
            "additionalProperties": false
          },
          "rightContent": {
            "type": "object",
            "properties": {
              "title": {
                "description": "Main heading. Max 6 words",
                "type": "string",
                "minLength": 18,
                "maxLength": 40
              },
              "sections": {
                "description": "List of content sections",
                "minItems": 1,
                "maxItems": 3,
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "description": "Section label text. Max 2 words",
                      "type": "string",
                      "minLength": 4,
                      "maxLength": 10
                    },
                    "value": {
                      "description": "Section value text. Max 100 characters",
                      "type": "string",
                      "minLength": 8,
                      "maxLength": 50
                    },
                    "showDivider": {
                      "description": "Whether to show bottom divider",
                      "type": "boolean"
                    }
                  },
                  "required": [
                    "label",
                    "value",
                    "showDivider"
                  ],
                  "additionalProperties": false
                }
              }
            },
            "required": [
              "title",
              "sections"
            ],
            "additionalProperties": false
          }
        },
        "required": [
          "metaMaxWords",
          "topBar",
          "leftPanel",
          "rightContent"
