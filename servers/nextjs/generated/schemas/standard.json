                  },
                  "__icon_query__": {
                    "description": "Query used to search the icon. Max 5 words",
                    "type": "string",
                    "minLength": 3,
                    "maxLength": 24
                  }
                },
                "required": [
                  "__icon_url__",
                  "__icon_query__"
                ],
                "additionalProperties": false
              },
              "websiteBar": {
                "type": "object",
                "properties": {
                  "websiteText": {
                    "description": "Website text in bottom green bar. Max 4 words",
