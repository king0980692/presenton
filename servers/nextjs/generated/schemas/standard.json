    {
      "id": "standard:chart-left-text-right-layout",
      "name": "Chart Left Text Right",
      "description": "A slide with header label, a left-side inline bar chart, and right-side title with paragraph.",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "title": {
            "description": "Main heading (max ~7 words)",
            "type": "string",
            "minLength": 16,
            "maxLength": 64
          },
          "paragraph": {
            "description": "Supporting description",
            "type": "string",
            "minLength": 50,
            "maxLength": 200
          },
          "chart": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "bar",
                  "horizontalBar",
                  "line",
                  "pie"
                ]
              },
              "data": {
                "minItems": 3,
                "maxItems": 8,
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "description": "Category label",
                      "type": "string",
                      "minLength": 1,
                      "maxLength": 12
                    },
                    "value": {
                      "description": "Value 0â€“100",
                      "type": "number",
                      "minimum": 0,
                      "maximum": 100
                    }
                  },
                  "required": [
                    "label",
                    "value"
                  ],
                  "additionalProperties": false
                }
              },
              "primaryColor": {
                "type": "string"
              },
              "gridColor": {
                "type": "string"
              },
              "pieColors": {
                "minItems": 1,
                "maxItems": 10,
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "showLabels": {
                "type": "boolean"
              }
            },
            "required": [
              "type",
              "data",
              "primaryColor",
              "gridColor",
              "pieColors",
              "showLabels"
            ],
            "additionalProperties": false
          }
        },
        "required": [
          "title",
          "paragraph",
          "chart"
        ],
        "additionalProperties": false
      }
    },
    {
